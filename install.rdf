<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">

    <Description about="urn:mozilla:install-manifest">
        <em:id>searchbuttonsbar@tumpio-post.com</em:id>
        <em:name>Search Buttons Bar</em:name>
        <em:description>A search engine toolbar with one-click search.</em:description>
        <em:version>1.2.2</em:version>
        <em:creator>tumpio</em:creator>
        <em:translator></em:translator>
        <em:homepageURL>https://github.com/tumpio/searchbuttonsbar</em:homepageURL>
        <em:iconURL>chrome://searchbuttonsbar/skin/icon.png</em:iconURL>
        <em:optionsURL>chrome://searchbuttonsbar/content/options.xul</em:optionsURL>
        <em:optionsType>1</em:optionsType>
        <em:multiprocessCompatible>true</em:multiprocessCompatible>
        <em:type>2</em:type>

        <!-- Pale Moon -->
        <em:targetApplication>
            <Description>
                <em:id>{8de7fcbb-c55c-4fbe-bfc5-fc555c87dbc4}</em:id>
                <em:minVersion>24.0</em:minVersion>
                <em:maxVersion>26.*</em:maxVersion>
            </Description>
        </em:targetApplication>

        <!-- Mozilla Firefox -->
        <em:targetApplication>
            <Description>
                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                <em:minVersion>4.0</em:minVersion>
                <em:maxVersion>49.*</em:maxVersion>
            </Description>
        </em:targetApplication>

        <!-- Locales -->
        <em:localized>
            <Description>
                <em:locale>en-US</em:locale>
                <em:name>Search Buttons Bar</em:name>
                <em:description>A search engine toolbar with one-click search.</em:description>
                <em:translator>tumpio</em:translator>
            </Description>
        </em:localized>
        <em:localized>
            <Description>
                <em:locale>fi</em:locale>
                <em:name>Search Buttons Bar</em:name>
                <em:description>Hakukonetyökalupalkki yhden klikkauksen hakuihin.</em:description>
                <em:translator>tumpio</em:translator>
            </Description>
        </em:localized>
        <em:localized>
            <Description>
                <em:locale>es</em:locale>
                <em:name>Search Buttons Bar</em:name>
                <em:description>Una barra de herramientas de motores de búsqueda, con búsqueda en un-clic.</em:description>
                <em:translator>strel</em:translator>
            </Description>
        </em:localized>

        <!-- Contributors -->
        <em:contributor>Brandon Carter</em:contributor>
        <em:contributor>James Davis</em:contributor>

    </Description>
</RDF>
